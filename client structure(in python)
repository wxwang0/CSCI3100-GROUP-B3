import socket
 
client = socket.socket()
client.connect(('--',9999))
 
client.send("Hello world.".encode())
client.close()

class ChatClient:
    def __init__(self):
        pass
 
    def start(self):
        pass
 
    def _recv(self):
        pass
 
    def send(self):
        pass
 
    def stop(self):
        pass
